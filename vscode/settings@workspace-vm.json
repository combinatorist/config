{
    "C_Cpp.autoAddFileAssociations": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "Meslo LG S, Menlo, DejaVu Sans Mono, monospace",
    "editor.fontSize": 13,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.minimap.showSlider": "always",
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [80],
    "editor.scrollBeyondLastColumn": 2,
    "editor.snippetSuggestions": "top",
    "elixirLS.dialyzerWarnOpts": [
        "unmatched_returns",
        "error_handling"
    ],
    "explorer.confirmDragAndDrop": false,
    "extensions.autoUpdate": false,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.associations": {
        "erlinit.config": "shellscript",
        "sxhkdrc": "properties",
        "vimrc": "viml",
        "vm.args": "shellscript",
        "vm.args.eex": "shellscript",
        "zshrc": "shellscript",
        ".clang-format": "yaml",
        ".envrc": "shellscript",
        ".gitignore.eex": "gitignore",
        "*.ex.eex": "elixir",
        "*.exs.eex": "elixir",
        "*.md.eex": "markdown",
        "*.nix.eex": "nix",
        "*.toml.eex": "toml"
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "flow.useNPMPackagedFlow": true,
    "git.confirmSync": false,
    "latex-workshop.chktex.args.active": [
        "-wall",
        "-n1",
        "-n13",
        "-n19",
        "-n22",
        "-n30",
        "-n44",
        "-e16",
        "-q"
    ],
    "latex-workshop.chktex.args.root": [
        "-wall",
        "-n1",
        "-n13",
        "-n19",
        "-n22",
        "-n30",
        "-n44",
        "-e16",
        "-q"
    ],
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.aux.bak",
        "*.bbl",
        "*.bcf",
        "*.blg",
        "*.brf",
        "*.dvi",
        "*.fdb_latexmk",
        "*.fls",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.glsdefs",
        "*.idx",
        "*.idx.bak",
        "*.ilg",
        "*.ind",
        "*.lof",
        "*.log",
        "*.lol",
        "*.lot",
        "*.nav",
        "*.out",
        "*.pdf",
        "*.ps",
        "*.snm",
        "*.synctex.gz",
        "*.toc",
        "*.tns",
        "*.vrb",
        "*.xdy",
        "_minted-*/*",
        "_minted-*/",
        "sage-plots-for-*.tex",
        "missfont.log",
        "texput.log",
        "texput.aux"
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "rust.clippy_preference": "on",
    "rust-client.channel": "stable",
    "search.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.history": true,
        "**/node_modules": true,
        "**/bower_components": true
    },
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "todohighlight.keywords":[
        {
            "text": "NOTE:",
            "color": "white",
            "backgroundColor": "#3333cc"
        },
        {
            "text": "IDEA:",
            "color": "white",
            "backgroundColor": "green"
        },
        {
            "text": "REVIEW:",
            "color": "white",
            "backgroundColor": "#880088"
        },
        {
            "text": "HACK:",
            "color": "white",
            "backgroundColor": "#cc3333"
        }
    ],
    "trailing-spaces.highlightCurrentLine": false,
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-c>": false,
        "<C-k>": false,
        "<C-n>": false,
        "<C-t>": false,
        "<C-v>": false,
        "<C-w>": false
    },
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        { "before": ["é"], "after": ["w"] },
        { "before": ["É"], "after": ["W"] },
        { "before": ["d", "a", "é"], "after": ["d", "a", "w"] },
        { "before": ["d", "a", "É"], "after": ["d", "a", "W"] },
