        { "before": ["l", "i", "é"], "after": ["l", "i", "w"] },
        { "before": ["l", "i", "É"], "after": ["l", "i", "W"] },
        { "before": ["w"], "after": ["<C-w>"] },
        { "before": ["W"], "after": ["<C-w>", "<C-w>"] },
        { "before": ["c"], "after": ["h"] },
